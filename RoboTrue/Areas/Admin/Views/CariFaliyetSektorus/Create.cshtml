@model RoboTrue.Core.Entities.CariFaliyetSektoru

@{
    ViewData["Title"] = "Create";
}



<h4>CariFaliyetSektoru</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="FaliyetSektoru" class="control-label"></label>
                <input asp-for="FaliyetSektoru" class="form-control" />
                <span asp-validation-for="FaliyetSektoru" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="OlusturanKullaniciId" class="control-label"></label>
                <select asp-for="OlusturanKullaniciId" class="form-control" asp-items="ViewBag.KullaniciId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Oluştur" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Geri Dön</a>
</div>

